{
	"C_Cpp.autoAddFileAssociations": false,
	"files.associations": {
		"*.s": "gas",
	},
	"commentAnchors.tags.anchors": {
		"ANCHOR": {
			"iconColor": "default",
			"highlightColor": "#A8C023",
			"scope": "file"
		},
		"TODO": {
			"iconColor": "blue",
			"highlightColor": "#3ea8ff",
			"scope": "workspace"
		},
		"FIXME": {
			"iconColor": "red",
			"highlightColor": "#F44336",
			"scope": "workspace"
		},
		"STUB": {
			"iconColor": "purple",
			"highlightColor": "#BA68C8",
			"scope": "file"
		},
		"NOTE": {
			"iconColor": "orange",
			"highlightColor": "#FFB300",
			"scope": "file"
		},
		"REVIEW": {
			"iconColor": "green",
			"highlightColor": "#64DD17",
			"scope": "workspace"
		},
		"SECTION": {
			"iconColor": "blurple",
			"highlightColor": "#896afc",
			"scope": "workspace",
			"behavior": "region",
			"enabled": false
		},
		"LINK": {
			"iconColor": "#2ecc71",
			"highlightColor": "#2ecc71",
			"scope": "workspace",
			"behavior": "link"
		},
		"IDEA": {
			"iconColor": "#3565ff",
			"highlightColor": "#3565ff",
			"scope": "workspace",
		}
	},

	"commentAnchors.workspace.excludeFiles": "**/{.git,.vscode,bin,sysroot,out,build,isodir,util}/**/*",
	"c-cpp-flylint.flexelint.enable": false,
	"c-cpp-flylint.clang.extraArgs": [
		"-DCONFIG_KERN_VERSION_MAJOR=0",
		"-DCONFIG_KERN_VERSION_MINOR=0",
		"-DCONFIG_KERN_VERSION_PATCH=6",
		"-I./include",
		"-std=gnu2x",
		"-ffreestanding",
		"-march=core2",
		"-mno-3dnow",
		"-mno-mmx"
	],
	"c-cpp-flylint.clang.language": "c",
	"c-cpp-flylint.cppcheck.extraArgs": [
		"--enable=all",
		"--std=c2x",
		"-I./include"
	],
	"c-cpp-flylint.flawfinder.enable": false,
	"C_Cpp.errorSquiggles": "enabled",
	"search.exclude": {
		"**/bin": true,
		"**/build": true,
		"**/isodir": true,
		"**/lib": true,
		"**/sysroot": true,
		"**/tmp": true,
		"**/util/mkbootimg": true
	},
	"vim.useCtrlKeys": false,
	"cSpell.ignoreWords": [
		"xchgw",
		"DUMPREGS",
		"bootloader",
		"scancode",
		"scancodes",
		"cppcheck",
		"scanline",
		"NUMLOCK",
		"Techflash",
		"Panicing",
		"utoa",
		"Framebuffer"
	],
	"editor.suggest.snippetsPreventQuickSuggestions": false,
	"aiXcoder.showTrayIcon": true,
	"cSpell.words": [
		"ABGR",
		"APIC",
		"bcol",
		"BOOTPROGRESSBAR",
		"bspid",
		"CAPSLOCK",
		"charc",
		"Chmela",
		"cmpb",
		"COREBOOT",
		"cpuid",
		"dgray",
		"disksize",
		"DLAB",
		"ebda",
		"EIMAG",
		"ELFLOAD",
		"hcol",
		"iconconv",
		"imread",
		"isodir",
		"kbps",
		"lblue",
		"lcyan",
		"lgray",
		"lgreen",
		"libc",
		"lidt",
		"lred",
		"Luk√°s",
		"MAXARGS",
		"memsz",
		"mkrescue",
		"MMIO",
		"numcores",
		"phtable",
		"putc",
		"quietfanatic",
		"ramdisk",
		"RECOMPOSER",
		"riscv",
		"Scanlines",
		"SCROLLLOCK",
		"serialwrite",
		"smbi",
		"superh",
		"sysroot",
		"tfos",
		"UEFI",
		"Warray",
